    "electron:osx": "electron-packager ./dist/desktop/mainnet/min/ --icon=./electron/icons/icon.icns --platform=darwin --out=./dist/osx",
    "electron": "gulp all && npm run electron:linux && npm run electron:win && npm run electron:osx",
